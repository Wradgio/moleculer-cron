  "keywords": [
    "microservice",
    "moleculer",
    "cron"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:davidroman0O/moleculer-cron.git"
  },
  "homepage": "https://github.com/davidroman0O/moleculer-cron#readme",
  "author": "David Roman",
  "peerDependencies": {
    "moleculer": "^0.14.12"
  },
  "devDependencies": {
    "benchmarkify": "^2.1.3",
    "coveralls": "3.1.0",
    "eslint": "7.23.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "lolex": "^6.0.0",
    "moleculer": "^0.14.12",
    "moleculer-docgen": "^0.2.2",
    "nodemon": "^2.0.7",
    "npm-check": "^5.9.2"
  },
  "jest": {
    "testEnvironment": "node",
